@model IEnumerable<AssignmentEAP.Models.DisciplineDefault>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_CustomLayout.cshtml";
}


<section class="py-5">
    <div class="row">

        <div class="col-lg-12 mb-6">
            <div class="card">
                <div class="card-header">
                    <h6 class="text-uppercase mb-0">Default Discipline Value </h6>
                </div>
                <div class="card-body">
                    <table class="table table-striped table-hover card-text data-table">
                        <thead>
                            <tr>
                                <th class="text-center">
                                    @Html.DisplayNameFor(model => model.Money)
                                </th>
                                <th class="text-center">
                                    @Html.DisplayNameFor(model => model.Squat_Amout)
                                </th>
                                <th class="text-center">
                                    @Html.DisplayNameFor(model => model.Push_Up)
                                </th>
                              
                                <th class="text-center">Edit</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td class="text-center">
                                        @Html.DisplayFor(modelItem => item.Money)
                                    </td>
                                    <td class="text-center">
                                        @Html.DisplayFor(modelItem => item.Squat_Amout)
                                    </td>
                                    <td class="text-center">
                                        @Html.DisplayFor(modelItem => item.Push_Up)
                                    </td>
                                    
                                    <td class="text-center">
                                        @Html.ActionLink("Edit", "Edit", new { id = item.DisciplineDefaultId }, new { @class = "btn btn-primary" })
                                      
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
</section>
